{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    "global-require": 0,
    "no-console": "off",
    "consistent-return": "off"
  }
}
